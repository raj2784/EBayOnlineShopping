
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PaymentFailurView</title>
</head>
<body>
    <div>
        <link href="~/Content/Success_Failer.css" rel="stylesheet" />

        <link href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
        <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js"></script>
        <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
        <!------ Include the above in your HEAD tag ---------->
        <div class="container m-auto text-center">
            <div class="row">
                <div class="col-md-12">
                    <br />
                    <br />
                    <div class="error-template">
                        <h1 class="label label-warning" style="font-size: 35px;">
                            Payment is unsuccessfull
                        </h1>
                        <br />
                        <h4 class="label label-warning" style="font-size: 20px;">
                            Please try again!
                        </h4>
                          
                        <br />
                        <br />
                        <br />
                        <div class="error-actions">
                            <a href="@Url.Action("Index", "Home")" class="btn btn-dark btn-lg">
                                <span class="glyphicon glyphicon-home"></span>
                                Back To  Home
                            </a>
                            <a href="#" class="btn btn-dark btn-lg"><span class="glyphicon glyphicon-envelope"></span> Contact Support </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</body>
</html>
